<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="769.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoginSignup.Register">
   <children>
      <Label fx:id="RegisterTitle" layoutX="259.0" layoutY="66.0" text="REGISTER FOR AN ACCOUNT">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label fx:id="SignUpUsername" layoutX="252.0" layoutY="174.0" text="Username" />
      <Label fx:id="SignUpPassword" layoutX="252.0" layoutY="208.0" text="Password" />
      <Label fx:id="ConfirmPassword" layoutX="204.0" layoutY="246.0" text="Confirm Password" />
      <TextField fx:id="SignUpUsernameField" layoutX="331.0" layoutY="169.0" />
      <Button fx:id="RegisterButton" layoutX="337.0" layoutY="299.0" mnemonicParsing="false" onMouseClicked="#registerClicked" text="Register" />
      <Label fx:id="ErrorMessage" layoutX="-2.0" layoutY="341.0" prefHeight="23.0" prefWidth="770.0" text=" " textAlignment="CENTER" />
      <Label fx:id="Email" layoutX="273.0" layoutY="138.0" text="Email" />
      <TextField fx:id="EmailField" layoutX="331.0" layoutY="135.0" />
      <Button fx:id="LogInButton" layoutX="14.0" layoutY="23.0" mnemonicParsing="false" onMouseClicked="#LogInClicked" text="Log In" />
      <PasswordField fx:id="SignUpPasswordField" layoutX="331.0" layoutY="204.0" />
      <PasswordField fx:id="ConfirmPasswordField" layoutX="331.0" layoutY="242.0" />
   </children>
</AnchorPane>
